{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Turing",
            "type": "node",
            "request": "launch",
            "runtimeArgs": ["-r", "ts-node/register"],
            "runtimeExecutable": "node",
            "args": ["--inspect", "${workspaceFolder}/turing.ts"],
            "cwd": "${workspaceRoot}",
            "skipFiles": ["<node_internals>/**", "node_modules/**"],
            "console": "integratedTerminal"
        }
    ],
}